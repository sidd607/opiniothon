            <output>
                <prompt selectionType="RANDOM">
                    <item>Hello! I am ZenBot, Your personal banking assistant. How may I help you today? </item>
                </prompt>
                <goto ref="getUserInput_2449614"/>
            </output>
            <getUserInput id="getUserInput_2449614">
                <search ref="folder_2449611"/>
                <default>
                    <output>
                        <prompt selectionType="RANDOM">
                         
                            <item>I am sorry, My knowledge is limited. I don't know the answer. would you like to connect to a customer care representative?</item>
                            <item>I dont understand what you are asking. Would you like to talk with a customer care representative?</item>
                        </prompt>
                    </output>
                </default>
            </getUserInput>
